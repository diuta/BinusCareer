    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "air-datepicker": "^3.5.3",
    "axios": "^1.4.0",
    "bootstrap": "4.4",
    "buffer": "^6.0.3",
    "concurrently": "^9.1.2",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "formik": "^2.4.6",
    "framer-motion": "^10.12.11",
    "jodit-react": "^5.2.19",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lodash.uniqby": "^4.7.0",
    "notistack": "^3.0.1",
    "prop-types": "^15.8.1",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-datepicker": "^7.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.2.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.0.5",
